    "prepare": "husky"
  },
  "dependencies": {
    "nuxt": "^3.12.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.7.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "3.3.2",
    "typescript": "~5.4.5",
    "typescript-eslint": "^7.14.1",
    "vue-tsc": "^1.8.27"
  },
  "overrides": {
    "eslint": "^9.6.0"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "npm run lint"
  }
